		"prisma": "6.16.2",
		"tsup": "8.5.0",
		"tsx": "4.20.6",
		"typescript": "5.9.2",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.2.4"
	},
	"dependencies": {
		"@fastify/cors": "11.1.0",
		"@fastify/swagger": "9.5.2",
		"@fastify/swagger-ui": "5.2.3",
		"@prisma/client": "6.16.2",
		"@scalar/fastify-api-reference": "1.38.1",
		"bcryptjs": "3.0.2",
		"dayjs": "1.11.18",
